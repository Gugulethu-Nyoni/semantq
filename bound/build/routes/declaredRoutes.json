[
  {
    "path": "user/:id/profile",
    "page": "user/profile"
  },
  {
    "path": "/",
    "component": "Home",
    "exact": true
  },
  {
    "path": "/about",
    "component": "About"
  },
  {
    "path": "/blog",
    "component": "Blog",
    "children": [
      {
        "path": ":postId",
        "component": "Post"
      }
    ]
  },
  {
    "path": "/contact",
    "component": "Contact"
  }
]
